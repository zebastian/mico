          }
        }
      }
    }
    restricted_resources = {
    }
    environmental_attributes = {
      PrinAttribute("SL3:TransportMechanism","TLS")
      PrinAttribute("SL3:LocalAddress","127.0.0.1")
      PrinAttribute("SL3:LocalPort","2345")
    }
    accepts_endorsement = false
